{
    "schemaName": "customer",
    "dimensionFieldSpecs": [
        {
            "name": "custkey",
            "dataType": "LONG"
        },
        {
            "name": "name",
            "dataType": "STRING"
        },
        {
            "name": "address",
            "dataType": "STRING"
        },
        {
            "name": "nationkey",
            "dataType": "LONG"
        },
        {
            "name": "phone",
            "dataType": "STRING"
        },
        {
            "name": "acctbal",
            "dataType": "DOUBLE"
        },
        {
            "name": "mktsegment",
            "dataType": "STRING"
        },
        {
            "name": "comment",
            "dataType": "STRING"
        }
    ],
    "dateTimeFieldSpecs": [
        {
            "name": "updated_at_seconds",
            "dataType": "LONG",
            "defaultNullValue": 0,
            "format": "1:SECONDS:EPOCH",
            "transformFunction": "toEpochSeconds(updated_at)",
            "granularity": "1:SECONDS"
        }
    ]
}
